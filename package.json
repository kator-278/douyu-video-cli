{
  "name": "douyu-cli",
  "version": "0.4.0",
  "description": "斗鱼视频下载CLI",
  "homepage": "https://github.com/renmu123/douyu-video-cli",
  "type": "module",
  "scripts": {
    "dev": "rollup --config rollup-dev.config.js -w",
    "build": "rollup --config rollup-dev.config.js",
    "publish": "pnpm run build && pnpm publish --access=public",
    "build:win": "rollup --config rollup-build-exe.config.js && node -v && node --experimental-sea-config sea-config.json && node -e \"require('fs').copyFileSync(process.execPath, 'douyu.exe')\" && npx postject douyu.exe NODE_SEA_BLOB sea-prep.blob --sentinel-fuse NODE_SEA_FUSE_fce680ab2cc467b6e072b8b5df1996b2 "
  },
  "keywords": [
    "douyu",
    "download",
    "cli",
    "video",
    "斗鱼",
    "视频",
    "下载",
    "直播",
    "live"
  ],
  "author": "renmu123",
  "license": "GPLv3",
  "engines": {
    "node": ">=18"
  },
  "bin": {
    "douyu": "bin/command.js"
  },
  "files": [
    "bin"
  ],
  "dependencies": {
    "axios": "^1.7.0"
  },
  "devDependencies": {
    "@kineticcafe/rollup-plugin-delete": "3.0.0-beta.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/cli-progress": "^3.11.5",
    "@types/crypto-js": "^4.2.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "20.10.0",
    "cli-progress": "^3.12.0",
    "commander": "^12.1.0",
    "crypto-js": "^4.2.0",
    "fast-xml-parser": "^4.4.0",
    "fs-extra": "^11.2.0",
    "hlsdownloader": "^3.2.5",
    "m3u8-parser": "^7.1.0",
    "rollup": "^4.17.2",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.1.1",
    "safe-eval": "^0.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "winston": "^3.13.0"
  }
}
